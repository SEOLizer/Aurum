// Comprehensive test for Math builtins
fn main(): int64 {
  print_str("=== Math Builtin Tests ===\n");
  
  // Test 1: Integer absolute value
  print_str("Test 1: Integer absolute value\n");
  var neg_val: int64 := -42;
  var pos_val: int64 := 37;
  print_str("abs(-42) = ");
  print_int(abs(neg_val));
  print_str("\nabs(37) = ");
  print_int(abs(pos_val));
  print_str("\n\n");
  
  // Test 2: Float absolute value
  print_str("Test 2: Float absolute value\n");
  var neg_float: f64 := -3.14;
  var abs_result: f64 := fabs(neg_float);
  print_str("fabs(-3.14) = ");
  PrintFloat(abs_result);
  print_str("\n\n");
  
  // Test 3: Square root and square
  print_str("Test 3: Square operations\n");
  var val: f64 := 4.0;
  var sqrt_result: f64 := sqrt(val);
  var sqr_result: f64 := sqr(val);
  print_str("sqrt(4.0) = ");
  PrintFloat(sqrt_result);
  print_str("\nsqr(4.0) = ");
  PrintFloat(sqr_result);
  print_str("\n\n");
  
  // Test 4: Rounding and truncation
  print_str("Test 4: Rounding and truncation\n");
  var float_val: f64 := 3.7;
  var rounded: int64 := round(float_val);
  var truncated: int64 := trunc(float_val);
  print_str("round(3.7) = ");
  print_int(rounded);
  print_str("\ntrunc(3.7) = ");
  print_int(truncated);
  print_str("\n\n");
  
  // Test 5: Fractional part
  print_str("Test 5: Fractional part\n");
  var frac_val: f64 := 5.25;
  var frac_result: f64 := frac(frac_val);
  print_str("frac(5.25) = ");
  PrintFloat(frac_result);
  print_str("\n\n");
  
  // Test 6: Pi constant
  print_str("Test 6: Pi constant\n");
  var pi_val: f64 := pi();
  print_str("pi() = ");
  PrintFloat(pi_val);
  print_str("\n\n");
  
  // Test 7: Bit operations
  print_str("Test 7: Bit operations\n");
  var test_val: int64 := 1311768467463790320;
  var high_bits: int64 := hi(test_val);
  var low_bits: int64 := lo(test_val);
  var swapped: int64 := swap(test_val);
  print_str("Original value (hex): ");
  print_int(test_val);
  print_str("\nhi() = ");
  print_int(high_bits);
  print_str("\nlo() = ");
  print_int(low_bits);
  print_str("\nswap() = ");
  print_int(swapped);
  print_str("\n\n");
  
  // Test 8: Odd/Even test
  print_str("Test 8: Odd/Even test\n");
  var even_num: int64 := 42;
  var odd_num: int64 := 37;
  print_str("odd(42) = ");
  if (odd(even_num)) { print_str("true"); } else { print_str("false"); }
  print_str("\nodd(37) = ");
  if (odd(odd_num)) { print_str("true"); } else { print_str("false"); }
  print_str("\n\n");
  
  // Test 9: Random number (simplified)
  print_str("Test 9: Random number\n");
  var rand_val: f64 := random();
  print_str("random() = ");
  PrintFloat(rand_val);
  print_str("\n\n");
  
  // Test 10: Trigonometric functions (simplified)
  print_str("Test 10: Trigonometric functions (simplified)\n");
  var angle: f64 := 1.0;
  var sin_result: f64 := sin(angle);
  var cos_result: f64 := cos(angle);
  print_str("sin(1.0) = ");
  PrintFloat(sin_result);
  print_str("\ncos(1.0) = ");
  PrintFloat(cos_result);
  print_str("\n");
  
  print_str("=== All Math tests completed! ===\n");
  
  return 0;
}