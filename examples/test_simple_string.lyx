// Einfacher Test f√ºr String-Builtins
fn main(): int64 {
  // Test 1: StrLength
  var len: int64 := StrLength("Hello");
  PrintStr("Length: ");
  PrintInt(len);
  PrintStr("\n");
  
  // Test 2: StrCharAt 
  var c1: int64 := StrCharAt("Hello", 0);
  var c2: int64 := StrCharAt("Hello", 1);
  PrintStr("First char: ");
  PrintInt(c1);
  PrintStr(", Second char: ");
  PrintInt(c2);
  PrintStr("\n");
  
  return 0;
}