// examples/use_env.lyx
import std.env;
import std.io;

fn main(argc: int64, argv: pchar): int64 {
  init(argc, argv);
  // Print arg count
  print_intln(arg_count());
  // Print first arg (program name) if present
  if arg_count() > 0 {
    print_str(arg(0));
    print_str("\n");
  }
  return 0;
}
