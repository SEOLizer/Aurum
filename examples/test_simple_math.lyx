// Simple test for Math builtins (integer only)
fn main(): int64 {
  PrintStr("=== Simple Math Builtin Tests ===\n");
  
  // Test 1: Integer absolute value
  PrintStr("Test 1: abs() function\n");
  var neg_val: int64 := -42;
  var pos_val: int64 := 37;
  var zero_val: int64 := 0;
  
  PrintStr("abs(-42) = ");
  PrintInt(abs(neg_val));
  PrintStr("\n");
  
  PrintStr("abs(37) = ");
  PrintInt(abs(pos_val));
  PrintStr("\n");
  
  PrintStr("abs(0) = ");
  PrintInt(abs(zero_val));
  PrintStr("\n\n");
  
  // Test 2: Odd/Even test
  PrintStr("Test 2: odd() function\n");
  var even_num: int64 := 42;
  var odd_num: int64 := 37;
  var zero_num: int64 := 0;
  
  PrintStr("odd(42) = ");
  if (odd(even_num)) { PrintStr("true"); } else { PrintStr("false"); }
  PrintStr("\n");
  
  PrintStr("odd(37) = ");
  if (odd(odd_num)) { PrintStr("true"); } else { PrintStr("false"); }
  PrintStr("\n");
  
  PrintStr("odd(0) = ");
  if (odd(zero_num)) { PrintStr("true"); } else { PrintStr("false"); }
  PrintStr("\n\n");
  
  // Test 3: Bit operations
  PrintStr("Test 3: Bit operations\n");
  var test_val: int64 := 123456;
  
  PrintStr("Original value: ");
  PrintInt(test_val);
  PrintStr("\n");
  
  PrintStr("hi(123456) = ");
  PrintInt(hi(test_val));
  PrintStr("\n");
  
  PrintStr("lo(123456) = ");
  PrintInt(lo(test_val));
  PrintStr("\n");
  
  PrintStr("swap(123456) = ");
  PrintInt(swap(test_val));
  PrintStr("\n\n");
  
  PrintStr("=== Simple Math tests completed! ===\n");
  
  return 0;
}