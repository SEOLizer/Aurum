// Comprehensive test of all string builtins
fn main(): int64 {
  print_str("=== Comprehensive String Builtin Tests ===\n");
  
  // Test 1: str_length
  print_str("Test 1: str_length\n");
  print_str("Length of 'Test': ");
  print_int(str_length("Test"));
  print_str("\n");
  
  // Test 2: StrCharAt
  print_str("\nTest 2: StrCharAt\n");
  var test_str: pchar := "Hello";
  print_str("Characters of 'Hello': ");
  print_int(StrCharAt(test_str, 0)); print_str(" ");
  print_int(StrCharAt(test_str, 1)); print_str(" ");
  print_int(StrCharAt(test_str, 2)); print_str(" ");
  print_int(StrCharAt(test_str, 3)); print_str(" ");
  print_int(StrCharAt(test_str, 4)); print_str("\n");
  print_str("Should be: 72 101 108 108 111 (H e l l o)\n");
  
  // Test 3: str_compare (simplified version - always returns 0)
  print_str("\nTest 3: str_compare (simplified)\n");
  var cmp_result: int64 := str_compare("abc", "abc");
  print_str("str_compare('abc', 'abc'): ");
  print_int(cmp_result);
  print_str(" (simplified implementation - always 0)\n");
  
  print_str("\n=== All basic tests completed! ===\n");
  
  return 0;
}