// Test f√ºr String-Builtins
fn main(): int64 {
  // Test 1: StrLength
  var len: int64 := StrLength("Hello");
  PrintStr("Length of Hello: ");
  PrintInt(len);
  PrintStr("\n");
  
  // Test 2: StrCharAt
  var c: int64 := StrCharAt("Hello", 1);
  PrintStr("Second character of Hello: ");
  PrintInt(c);
  PrintStr(" (ASCII)\n");
  
  // Test 3: str_compare
  var cmp: int64 := str_compare("abc", "abc");
  PrintStr("Compare abc with abc: ");
  PrintInt(cmp);
  PrintStr(" (should be 0)\n");
  
  var cmp2: int64 := str_compare("abc", "def");
  PrintStr("Compare abc with def: ");
  PrintInt(cmp2);
  PrintStr(" (should be negative)\n");
  
  return 0;
}