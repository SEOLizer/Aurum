// Beispiel: Unit- und Import-Deklarationen
// Dies ist ein Beispiel für die Syntax (Parsing-Only)

unit example.math;

import std.io;

// Öffentliche Funktion
pub fn add(a: int64, b: int64): int64 {
    return a + b;
}

fn main(): int64 {
    var result: int64 := add(3, 4);
    print_int(result);
    print_str("\n");
    return 0;
}
